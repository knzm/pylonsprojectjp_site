<div id="feed-control">
  <span style="color:#676767;font-size:11px;margin:10px;padding:4px;">Loading...</span>
</div>
<script src="http://www.google.com/jsapi?key=notsupplied-wizard" type="text/javascript"></script>
<script src="http://www.google.com/uds/solutions/dynamicfeed/gfdynamicfeedcontrol.js" type="text/javascript"></script>
<style type="text/css">
  @import url("http://www.google.com/uds/solutions/dynamicfeed/gfdynamicfeedcontrol.css");
</style>
<script type="text/javascript">
  function loadFeedControl() {
    var feed = "http://pipes.yahoo.com/pipes/pipe.run?_id=318f0e48a6fce92de71b190042133c9f&_render=rss";
    var options = {
      stacked: false,
      horizontal: false,
      title: "RSS Feed"
    };
    new GFdynamicFeedControl(feed, "feed-control", options);
  };
  google.load("feeds", "1");
  google.setOnLoadCallback(loadFeedControl);
</script>
